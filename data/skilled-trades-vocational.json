    "meta": {
      "title": "Sports & Fitness",
      "category": "Skilled Trades & Vocational",
      "description": "Pursue a career as a professional athlete, coach, or fitness trainer.",
      "full_description": "The Sports and Fitness industry offers diverse roles from professional athletes representing the country to fitness trainers helping individuals stay healthy. It requires immense dedication, physical discipline, and continuous training.",
      "roadmap": [
        {
          "step": "Early Start",
          "details": "Start training in a specific sport during school days (Age 8-12).",
          "duration": "Childhood"
        },
        {
          "step": "District/State Level",
          "details": "Participate in district and state-level tournaments to get recognized.",
          "duration": "School/College"
        },
        {
          "step": "Professional Training",
          "details": "Join a sports academy (SAI) or get a certification (ACE, ACSM) for fitness training.",
          "duration": "1-3 Years"
        },
        {
          "step": "National Level",
          "details": "Represent the state at Nationals. For trainers, start working at gyms.",
          "duration": "Career Peak"
        },
        {
          "step": "Coach / Mentor",
          "details": "Transition to coaching after retirement or gain advanced certifications.",
          "duration": "Post-Career"
        }
